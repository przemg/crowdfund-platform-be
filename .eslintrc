{
  "extends": ["prettier", "airbnb-base"],
  "plugins": ["prettier"],
  "env": {
    "es2021": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": "error",
    "import/extensions": "off",
    "import/prefer-default-export": "off",
    "implicit-arrow-linebreak": ["error", "below"]
  }
}
